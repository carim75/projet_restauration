{% for type, messages in app.session.flashBag.all %}

    {% if type == 'error' %}
        {% set type='danger' %}
    {% endif %}
    {% for message in messages %}
        <div class="alert alert-{{ type }} mt-2">
            {{ message | raw }}
        </div>
    {% endfor %}

{% endfor %}



